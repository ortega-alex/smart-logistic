{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard",
        "eslint-config-prettier"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "no-unneeded-ternary": "off",
        "camelcase": "off",
        "react/prop-types": "off",
        "no-undef": "off",
        "react/react-in-jsx-scope": "off"
    }
}